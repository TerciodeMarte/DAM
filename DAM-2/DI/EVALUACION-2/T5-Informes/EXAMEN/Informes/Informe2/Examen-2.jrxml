	<queryString>
		<![CDATA[SELECT examen_tema5.clientes.`ID_Cliente`,
	examen_tema5.clientes.`Nombre` AS Empresa,
	examen_tema5.emails.`Email`,
	examen_tema5.telefonos.`Telefono`,
	examen_tema5.telefonos.`Persona_Contacto`,
	examen_tema5.pedidos.`ID_Pedido`,
	examen_tema5.pedidos.`Fecha_Pedido`,
	examen_tema5.articulos.`Descripcion` AS NameArticulo,
	examen_tema5.detalle_pedidos.`Unidades`,
	examen_tema5.detalle_pedidos.`Precio`,
	examen_tema5.detalle_pedidos.`Unidades` * examen_tema5.detalle_pedidos.`Precio` AS Importe
FROM examen_tema5.clientes
	inner join examen_tema5.emails USING(id_cliente) 
	inner join examen_tema5.telefonos USING(id_cliente) 
	inner join examen_tema5.pedidos USING(id_cliente) 
	inner join examen_tema5.detalle_pedidos USING(id_pedido) 
	inner join examen_tema5.articulos USING(id_articulo) 
WHERE 
 examen_tema5.articulos.`Descripcion`= "MSI NVIDIA RTX-4070 12GB GDDR6X"]]>
